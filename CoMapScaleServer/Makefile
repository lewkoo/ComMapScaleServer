####### Install

install_target: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/opt/CoMapScaleServer/bin/ || $(MKDIR) $(INSTALL_ROOT)/opt/CoMapScaleServer/bin/ 
	-$(INSTALL_PROGRAM) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/opt/CoMapScaleServer/bin/$(QMAKE_TARGET)"

uninstall_target:  FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/opt/CoMapScaleServer/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/CoMapScaleServer/bin/ 


install:  install_target  FORCE

uninstall: uninstall_target   FORCE

FORCE:

